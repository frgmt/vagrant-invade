    <% if uid %>
    <%= machine_name %>.winnfsd.uid = <%= uid %>
    <% else %>
    <%= machine_name %>.winnfsd.uid = Process.uid
    <% end %>
    <% if gid %>
    <%= machine_name %>.winnfsd.gid = <%= gid %>
    <% else %>
    <%= machine_name %>.winnfsd.gid = Process.gid
    <% end %>
    <% if logging %>
    <%= machine_name %>.winnfsd.logging = <%= logging %>
    <% end %>
