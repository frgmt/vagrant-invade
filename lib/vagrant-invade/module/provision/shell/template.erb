    <%= machine_name %>.vm.provision "shell",
    <% data.each_with_index do |(k, v), index| %>
      <% if k['args'] %>
      <%= k %>: <%= v %><% unless index == data.size - 1 %>,<% end %>
      <% else %>
      <%= k %>: '<%= v %>'<% unless index == data.size - 1 %>,<% end %>
      <% end %>
    <% end %>
